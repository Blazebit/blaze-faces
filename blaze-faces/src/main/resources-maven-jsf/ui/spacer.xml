<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
	<tag>spacer</tag>
	<componentClass>com.blazebit.blazefaces.component.spacer.Spacer</componentClass>
	<componentType>com.blazebit.blazefaces.component.Spacer</componentType>
	<componentFamily>com.blazebit.blazefaces.component</componentFamily>
	<rendererType>com.blazebit.blazefaces.component.SpacerRenderer</rendererType>
	<rendererClass>com.blazebit.blazefaces.component.spacer.SpacerRenderer</rendererClass>
	<parent>javax.faces.component.UIComponentBase</parent>
    <description>Spacer is used to put spaces between elements.</description>
	<attributes>
		<xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
		<attribute>
			<name>width</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Width of the space.</description>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Height of the space.</description>
		</attribute>
        <attribute>
			<name>title</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Advisory tooltip informaton.</description>
		</attribute>
        <attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Inline style of the spacer.</description>
		</attribute>
        <attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Style class of the spacer.</description>
		</attribute>
	</attributes>
</component>
