<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
	<tag>galleria</tag>
	<componentClass>com.blazebit.blazefaces.component.galleria.Galleria</componentClass>
	<componentType>com.blazebit.blazefaces.component.Galleria</componentType>
	<componentFamily>com.blazebit.blazefaces.component</componentFamily>
	<rendererType>com.blazebit.blazefaces.component.GalleriaRenderer</rendererType>
	<rendererClass>com.blazebit.blazefaces.component.galleria.GalleriaRenderer</rendererClass>
	<parent>com.blazebit.blazefaces.component.UIData</parent>
    <description>Galleria is used to display a set of images.</description>
    <interfaces>
		<interface>
			<name>com.blazebit.blazefaces.component.Widget</name>
		</interface>
	</interfaces>
	<attributes>
		<xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
		<xi:include href="./../entities/widget_var_attribute.xml" parse="text"/>
        <attribute>
            <name>value</name>
            <required>true</required>
            <type>java.lang.Object</type>
            <ignoreInComponent>true</ignoreInComponent>
            <description></description>
        </attribute>
        <attribute>
            <name>var</name>
            <required>true</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
            <description></description>
        </attribute>
         <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Inline style of the container element.</description>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Style class of the container element.</description>
        </attribute>
		<attribute>
			<name>effect</name>
			<required>false</required>
			<type>java.lang.String</type>
            <defaultValue>crossfade</defaultValue>
            <description>Name of animation to use on galleria switches. Default is crossfade.</description>
		</attribute>
        <attribute>
			<name>effectSpeed</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>800</defaultValue>
            <description>Duration of animation in milliseconds. Default is 800.</description>
		</attribute>
        <attribute>
			<name>panelWidth</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>600</defaultValue>
		</attribute>
        <attribute>
			<name>panelHeight</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>400</defaultValue>
		</attribute>
        <attribute>
			<name>frameWidth</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>60</defaultValue>
		</attribute>
        <attribute>
			<name>frameHeight</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>40</defaultValue>
		</attribute>
        <attribute>
			<name>filmstripStyle</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
        <attribute>
			<name>filmstripPosition</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
        <attribute>
			<name>showFilmstrip</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>true</defaultValue>
		</attribute>
        <attribute>
			<name>showCaptions</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
		</attribute>
        <attribute>
			<name>showOverlays</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
		</attribute>
        <attribute>
			<name>transitionInterval</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>4000</defaultValue>
		</attribute>
	</attributes>
	<resources>
        <resource>
			<name>galleria/galleria.css</name>
		</resource>
		<resource>
			<name>jquery/jquery.js</name>
		</resource>
		<resource>
			<name>blazefaces.js</name>
		</resource>
        <resource>
			<name>galleria/galleria.js</name>
		</resource>
