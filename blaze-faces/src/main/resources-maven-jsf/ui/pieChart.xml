<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
	<tag>pieChart</tag>
	<componentClass>com.blazebit.blazefaces.component.chart.pie.PieChart</componentClass>
	<componentType>com.blazebit.blazefaces.component.chart.PieChart</componentType>
	<componentFamily>com.blazebit.blazefaces.component</componentFamily>
	<rendererType>com.blazebit.blazefaces.component.chart.PieChartRenderer</rendererType>
	<rendererClass>com.blazebit.blazefaces.component.chart.pie.PieChartRenderer</rendererClass>
	<parent>com.blazebit.blazefaces.component.chart.UIChart</parent>
    <interfaces>
		<interface>
			<name>com.blazebit.blazefaces.component.Widget</name>
		</interface>
	</interfaces>
	<attributes>
		<xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
        <xi:include href="./../entities/widget_var_attribute.xml" parse="text"/>
		<attribute>
			<name>value</name>
			<required>false</required>
			<type>com.blazebit.blazefaces.model.chart.ChartModel</type>
			<ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>title</name>
			<required>false</required>
			<type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>legendPosition</name>
			<required>false</required>
			<type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>seriesColors</name>
			<required>false</required>
			<type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>diameter</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>java.lang.Integer.MIN_VALUE</defaultValue>
		</attribute>
        <attribute>
			<name>sliceMargin</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <defaultValue>0</defaultValue>
		</attribute>
        <attribute>
			<name>fill</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>true</defaultValue>
		</attribute>
        <attribute>
			<name>shadow</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
        <ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>showDataLabels</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
            <description>Whether data information will be showed on chart. Default is false.</description>
		</attribute>
        <attribute>
            <name>dataFormat</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Defines data information type on highlighter or slices. Default is percent.</description>
        </attribute>
        <attribute>
			<name>legendCols</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <ignoreInComponent>true</ignoreInComponent>
            <description>Defines visible column count of chart legend.</description>
		</attribute>
        <attribute>
			<name>legendRows</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <ignoreInComponent>true</ignoreInComponent>
            <description>Defines visible row count of chart legend.</description>
		</attribute>
        <attribute>
			<name>extender</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <ignoreInComponent>true</ignoreInComponent>
            <description>Javascript function to extend the chart with low level jqplot options.</description>
		</attribute>
	</attributes>
	<resources>
        <resource>
			<name>charts/charts.css</name>
		</resource>
		<resource>
			<name>jquery/jquery.js</name>
		</resource>
		<resource>
			<name>blazefaces.js</name>
		</resource>
		<resource>
			<name>charts/charts.js</name>
		</resource>
	</resources>
</component>