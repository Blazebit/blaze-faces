<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
	<tag>imageCompare</tag>
	<componentClass>com.blazebit.blazefaces.component.imagecompare.ImageCompare</componentClass>
	<componentType>com.blazebit.blazefaces.component.ImageCompare</componentType>
	<componentFamily>com.blazebit.blazefaces.component</componentFamily>
	<rendererType>com.blazebit.blazefaces.component.ImageCompareRenderer</rendererType>
	<rendererClass>com.blazebit.blazefaces.component.imagecompare.ImageCompareRenderer</rendererClass>
	<parent>javax.faces.component.UIComponentBase</parent>
    <description>ImageCompare provides a rich user interface to compare two images.</description>
    <interfaces>
		<interface>
			<name>com.blazebit.blazefaces.component.Widget</name>
		</interface>
	</interfaces>
    <attributes>
		<xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
        <xi:include href="./../entities/widget_var_attribute.xml" parse="text"/>
		<attribute>
			<name>leftImage</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Source of the image placed on the left side.</description>
		</attribute>
		<attribute>
			<name>rightImage</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Source of the image placed on the right side</description>
		</attribute>
		<attribute>
			<name>width</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Width of the images.</description>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Height of the images.</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Style of the image container element.</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Style class of the image container element.</description>
		</attribute>
	</attributes>
	<resources>
		<resource>
			<name>jquery/jquery.js</name>
		</resource>
        <resource>
			<name>blazefaces.js</name>
		</resource>
		<resource>
			<name>imagecompare/imagecompare.js</name>
		</resource>
	</resources>
</component>