<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
    <tag>panelGroup</tag>
    <componentClass>com.blazebit.blazefaces.component.panelgroup.PanelGroup</componentClass>
    <componentType>com.blazebit.blazefaces.component.PanelGroup</componentType>
    <componentFamily>com.blazebit.blazefaces.component</componentFamily>
    <rendererType>com.blazebit.blazefaces.component.PanelGroupRenderer</rendererType>
    <rendererClass>com.blazebit.blazefaces.component.panelgroup.PanelGroupRenderer</rendererClass>
    <parent>javax.faces.component.UIPanel</parent>
    <description>Panel is a grouping component for other components. The type of the panelGroup affects what HTML tag will be rendered.</description>
    <interfaces>
		<interface>
			<name>com.blazebit.blazefaces.component.Widget</name>
		</interface>
        <interface>
			<name>javax.faces.component.behavior.ClientBehaviorHolder</name>
		</interface>
	</interfaces>
    <attributes>
        <xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
        <xi:include href="./../entities/widget_var_attribute.xml" parse="text"/>
		<xi:include href="./../entities/html5_data_attributes.xml" parse="text"/>
        <attribute>
            <name>type</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description><![CDATA[The type of the panel that should be rendered.
            Valid values are:
            <ul>
                <li>div</li>
                <li>span</li>
                <li>article</li>
                <li>aside</li>
                <li>footer</li>
                <li>header</li>
                <li>hgroup</li>
                <li>nav</li>
                <li>section</li>
                <li>h1-h6</li>
                <li>address</li>
                <li>blockquote</li>
                <li>p</li>
                <li>code</li>
                <li>figure</li>
                <li>figcaption</li>
                <li>summary</li>
            </ul>]]>
            </description>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Style of the panelGroup.</description>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Style class of the panelGroup.</description>
        </attribute>
    </attributes>
    <resources>
        <resource>
			<name>core/html5.js</name>
			<target>head_lt_ie9</target>
		</resource>
		<resource>
			<name>core/innershiv.js</name>
		</resource>
	</resources>
</component>