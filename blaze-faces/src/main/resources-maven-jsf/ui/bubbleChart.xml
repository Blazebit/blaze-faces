        <attribute>
            <name>value</name>
            <required>false</required>
            <type>com.blazebit.blazefaces.model.chart.BubbleChartModel</type>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
