<?xml version="1.0" encoding="UTF-8"?>
<component  xmlns="http://blazebit.com/jsf/component"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://blazebit.com/jsf/component ../misc/component.xsd">
	<tag>treeNode</tag>
	<componentClass>com.blazebit.blazefaces.component.tree.UITreeNode</componentClass>
	<componentType>com.blazebit.blazefaces.component.UITreeNode</componentType>
	<componentFamily>com.blazebit.blazefaces.component</componentFamily>
	<parent>javax.faces.component.UIColumn</parent>
    <description>TreeNode is used with Tree component to represent a node in tree.</description>
	<attributes>
		<xi:include href="./../entities/standard_uicomponent_attributes.xml" parse="text"/>
		<attribute>
			<name>type</name>
			<required>false</required>
			<type>java.lang.String</type>
			<defaultValue>default</defaultValue>
            <description>Type of the tree node. Default is "default"</description>
		</attribute>
        <attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Style class to apply a particular tree node type.</description>
		</attribute>
		<attribute>
			<name>icon</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description></description>
		</attribute>
        <attribute>
			<name>expandedIcon</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description></description>
		</attribute>
        <attribute>
			<name>collapsedIcon</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description></description>
		</attribute>
	</attributes>
</component>
