        <behavior-class>com.blazebit.blazefaces.behavior.show.ShowBehavior</behavior-class>
    </behavior>
    <behavior>
        <behavior-id>com.blazebit.blazefaces.behavior.HideBehavior</behavior-id>
        <behavior-class>com.blazebit.blazefaces.behavior.hide.HideBehavior</behavior-class>
    </behavior>
    
    <component>
