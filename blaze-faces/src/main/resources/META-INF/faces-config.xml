            <renderer-type>com.blazebit.blazefaces.behavior.handler.EventHandlerRenderer</renderer-type>
            <renderer-class>com.blazebit.blazefaces.behavior.handler.EventHandlerRenderer</renderer-class>
        </renderer>
        
