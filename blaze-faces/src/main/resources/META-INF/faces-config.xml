            <component-family>com.blazebit.blazefaces.component</component-family>
            <renderer-type>com.blazebit.blazefaces.component.SelectOneListboxRenderer</renderer-type>
            <renderer-class>com.blazebit.blazefaces.component.selectonelistbox.SelectOneListboxRenderer</renderer-class>
        </renderer>
