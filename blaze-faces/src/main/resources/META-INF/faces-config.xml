        <behavior-class>com.blazebit.blazefaces.behavior.ajax.AjaxBehavior</behavior-class>
    </behavior>
    <behavior>
        <behavior-id>com.blazebit.blazefaces.behavior.InvokeBehavior</behavior-id>
        <behavior-class>com.blazebit.blazefaces.behavior.invoke.InvokeBehavior</behavior-class>
    </behavior>
    <behavior>
