            <component-family>com.blazebit.blazefaces.component</component-family>
            <renderer-type>com.blazebit.blazefaces.component.SelectOneMenuRenderer</renderer-type>
            <renderer-class>com.blazebit.blazefaces.component.selectonemenu.SelectOneMenuRenderer</renderer-class>
        </renderer>
